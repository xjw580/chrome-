{
	"manifest_version":2,
	"name":"skipB站Ad",
	"description":"按Z跳过B站的视频捐赠页面",
	"version":"1.0.1",
	"icons":{
		"16":"images/skip.png",
		"32":"images/skip.png",
		"48":"images/skip.png",
		"128":"images/skip.png"
	},
	"background":{
		"scripts":["js/bg.js"],
		"persistent":false
	},
	"content_scripts":[
		{
			"js":["js/jquery.min.js","js/content.js"],
			"matches":["*://www.bilibili.com/*"]
		}
	],
	"permissions":[
		"<all_urls>"
	]
}